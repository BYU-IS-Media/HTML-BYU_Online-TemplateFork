			.script, .rtl {
				font-size: 18px
			}
			.big {
				font-size: 21px
			}
			.rtl {
				direction: rtl;
				unicode-bidi: embed;
				text-indent: 3em;
				text-align: right;
				border-right: 1px solid #ddd;
				border-left: 1px solid #fff;
			}
			.lost {
